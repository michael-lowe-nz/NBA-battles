{
  "name": "NBA-battles",
  "version": "1.0.0",
  "description": "Compare your favourite players",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test/*.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michael-lowe-nz/NBA-battles"
  },
  "author": "michael-lowe-nz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michael-lowe-nz/NBA-battles/issues"
  },
  "homepage": "https://github.com/michael-lowe-nz/NBA-battles",
  "dependencies": {
    "nba": "^3.2.6",
    "react-autosuggest": "^9.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "clone": "^2.1.0",
    "deep-freeze": "0.0.1",
    "extend-tape": "^1.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.0.1",
    "react-md-spinner": "^0.2.1",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-unit": "^2.0.1",
    "superagent": "^3.5.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-jsx-equals": "^1.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  }
}
